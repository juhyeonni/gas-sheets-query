{
  "name": "gas-sheets-query",
  "version": "0.1.0",
  "description": "TypeScript library for using Google Sheets as a database in GAS projects",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.mjs && tsc --emitDeclarationOnly",
    "build:gas": "node build.mjs",
    "dev": "vitest",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "google-apps-script",
    "gas",
    "spreadsheet",
    "database",
    "query-builder"
  ],
  "author": "juhyeonni",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juhyeonni/gas-sheets-query.git",
    "directory": "packages/core"
  },
  "devDependencies": {
    "@types/google-apps-script": "^2.0.8",
    "@types/node": "^25.2.1",
    "esbuild": "^0.27.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
