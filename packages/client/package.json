{
  "name": "@gsquery/client",
  "version": "0.2.3",
  "description": "Typed client for gas-sheets-query",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./generated": {
      "types": "./generated/index.d.ts",
      "import": "./generated/index.js"
    }
  },
  "files": [
    "dist",
    "generated"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist generated"
  },
  "keywords": [
    "gas-sheets-query",
    "typed-client",
    "client",
    "database",
    "google-sheets"
  ],
  "author": "juhyeonni",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juhyeonni/gas-sheets-query.git",
    "directory": "packages/client"
  },
  "dependencies": {
    "@gsquery/core": "workspace:*"
  },
  "devDependencies": {
    "@types/google-apps-script": "^2.0.8",
    "@types/node": "^25.2.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
