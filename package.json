{
  "name": "gas-sheets-query-monorepo",
  "version": "0.3.0",
  "private": true,
  "description": "Monorepo for gas-sheets-query",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:coverage": "pnpm -r exec vitest run --coverage",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean",
    "dev": "pnpm --filter gsquery dev",
    "release": "pnpm build && pnpm -r publish --access public"
  },
  "keywords": [
    "google-apps-script",
    "gas",
    "spreadsheet",
    "database",
    "query-builder"
  ],
  "author": "juhyeonni",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juhyeonni/gas-sheets-query.git"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18"
  }
}
